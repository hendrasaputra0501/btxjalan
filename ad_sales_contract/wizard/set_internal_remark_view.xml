<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_internal_remark_wizard" model="ir.ui.view">
            <field name="name">Set Internal Remark</field>
            <field name="model">internal.remark.wizard</field>
            <field name="arch" type="xml">
            <form string="Set Internal Remark" version="7.0">
                <separator string="Set Internal Remark"/>
                <group>
                    <field name="remark"/>
                </group>
                <footer>
                    <button name="set_internal_remark" string="Set" type="object" class="oe_highlight" />
                    or
                    <button string="Cancel" class="oe_link" special="cancel" />
                </footer>
            </form>
            </field>
        </record>
        <record id="action_internal_remark_wizard" model="ir.actions.act_window">
            <field name="name">Set Internal Remark</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">internal.remark.wizard</field>
            <field name="view_type">form</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>

